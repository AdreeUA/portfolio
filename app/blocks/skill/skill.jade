include ../heading/heading

mixin skill()
	+b.skill&attributes(attributes)
		+e.list
			+e.item
				+e.use_css CSS (3)
					+e.SPAN.persent 89%
			+e.item
				+e.use_html HTML (5)
					+e.SPAN.persent 77%
			+e.item
				+e.use_js JavaScript
					+e.SPAN.persent 65%
			+e.item
				+e.use_ph Photoshop / Sketch
					+e.SPAN.persent 85%
			+e.item
				+e.use_cms CMS
					+e.SPAN.persent 65%
			+e.item
				+e.use_php PHP
					+e.SPAN.persent 30%
